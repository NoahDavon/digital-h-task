datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int     @id
  first_name String  @db.VarChar(50)
  last_name  String? @db.VarChar(50)
  email      String  @unique @db.VarChar(50)
  bio        String?
  pass_hash  String
}

generator client {
  provider = "prisma-client-js"
}